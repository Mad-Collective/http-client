{
    "name": "pluggit/http-client",
    "description": "A library to provide cache interface access to different backend caches",
    "type": "library",
    "require": {
        "php": ">=5.5",
        "psr/log": "^1.0",
        "guzzlehttp/psr7": "^1.0"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.5",
        "behat/behat": "^3.1",
        "phpunit/php-code-coverage": "^2.2",
        "guzzlehttp/guzzle": "^6.0",
        "symfony/console": "^3.1",
        "symfony/yaml": "^3.1",
        "pimple/pimple": "^3.0",
        "php-mock/php-mock": "^1.0"
    },
    "authors": [
        {
            "name": "Hilari Moragrega",
            "email": "hilari@hilarimoragrega.com"
        }
    ],
    "config": {
        "bin-dir": "bin"
    },
    "autoload": {
        "psr-4": {
            "Cmp\\Http\\": "src/",
            "features\\Cmp\\Http\\": "tests/features/bootstrap/",
            "spec\\Cmp\\Http\\": "tests/spec/",
            "bin\\Cmp\\Http\\": "bin/"
        }
    }
}
